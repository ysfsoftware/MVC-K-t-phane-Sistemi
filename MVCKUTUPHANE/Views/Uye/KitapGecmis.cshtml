@using MVCKUTUPHANE.Models.Entity
@model List<TBLHAREKET> 
@{
    ViewBag.Title = "KitapGecmis";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@ViewBag.uye1</h2>
<br />
<table class="table table-hover">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach(var x in Model)
    {
        <tr>
            <td>@x.TBLKITAP.AD</td>
            <td>@x.TBLPERSONEL.PERSONEL</td>
            <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(x.UYEGETIRTARIH).ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>

