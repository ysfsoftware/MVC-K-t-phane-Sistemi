@using MVCKUTUPHANE.Models.Entity
@model List<TBLKITAP>
@{
    ViewBag.Title = "YazarKitaplar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.y1</h2>
<br />
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Kitap Adı</th>
        <th>Kategori</th>
        <th>Yayınevi</th>
    </tr>
    @foreach(var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.AD</td>
            <td>@x.TBLKATEGORI.AD</td>
            <td>@x.YAYINEVI</td>
        </tr>
    }
</table>
